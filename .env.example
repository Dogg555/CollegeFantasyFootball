# Copy this file to .env.local (which is gitignored) and fill in your secrets.

# Postgres
POSTGRES_USER=your_pg_user
POSTGRES_PASSWORD=your_pg_password
POSTGRES_DB=cff
DB_URL=postgres://your_pg_user:your_pg_password@postgres:5432/cff

# Backend
JWT_SECRET=your_jwt_secret
PORT=8080

# TLS (mount certs dir via CERTS_DIR; set paths below to match container paths)
SSL_CERT_FILE=/certs/dev.crt
SSL_KEY_FILE=/certs/dev.key
CERTS_DIR=./certs

# CFBD ingestion
CFBD_API_KEY=your_cfbd_api_key
CFBD_SEASON=2024
CFBD_DELAY_SEC=0.2
